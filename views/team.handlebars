<div class="columns">
<div class="card">
  <div class="card-content">
    <div class="media">
      <div class="media-left">
        <figure class="image is-48x48">
          <img src="https://bulma.io/images/placeholders/96x96.png" alt="Placeholder image">
        </figure>
      </div>
      {{#each dogWalker as |walker|}}
      <div class="media-content">
        <p class="title is-4">{{walker.name}}</p>
        <p class="subtitle is-6">{{walker.email}}</p>
      </div>
    </div>

    <div class="content">
      {{walker.description}}  
    </div>
    {{/each}}
  </div>
</div>
</div>