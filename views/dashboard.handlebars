



<section>
  <h2>Dog Walkers</h2>
  <table>

  <thead>
    <tr>
      <th>Dog Name</th>
      <th>Dog ID</th>
      <th>Owner ID</th>
      <th>Walker ID</th>
    </tr>
  </thead>
  <tbody>
    {{#each dogs as |dog|}}
      <tr class="odd:bg-white even:bg-slate-100">
        <td>{{dog.name}}</td>
        <td>{{dog.id}}</td>
        <td>{{dog.owner.id}}</td>
        <td>{{dog.dogwalker.id}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
</section>



